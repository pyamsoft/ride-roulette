{
  "name": "ride-roulette",
  "version": "0.4.3",
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "jotai": "2.16.1",
    "react": "19.2.3",
    "react-confetti": "6.4.0",
    "react-dom": "19.2.3",
    "react-window": "2.2.4",
    "uuid": "13.0.0"
  },
  "scripts": {
    "start": "vite",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "lint:stylelint": "stylelint 'src/**/*.css'",
    "lint": "$npm_execpath run lint:eslint && $npm_execpath run lint:stylelint && $npm_execpath run lint:tsc",
    "build": "$npm_execpath run lint && vite build",
    "build:preview": "$npm_execpath run build && $npm_execpath run preview",
    "preview": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@jest/globals": "30.2.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "eslint": "9.39.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "17.0.0",
    "jsdom": "27.4.0",
    "npm-check-updates": "19.3.1",
    "prettier": "3.7.4",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vite": "7.3.0",
    "vitest": "4.0.16",
    "web-vitals": "5.1.0"
  },
  "packageManager": "pnpm@10.27.0"
}

{
  "name": "ride-roulette",
  "version": "0.4.1",
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "jotai": "2.12.5",
    "react": "19.1.0",
    "react-confetti": "6.4.0",
    "react-dom": "19.1.0",
    "react-window": "1.8.11",
    "uuid": "11.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "$npm_execpath run lint && tsc && vite build",
    "lint": "$npm_execpath run eslint && $npm_execpath run stylelint",
    "eslint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "stylelint": "stylelint 'src/**/*.css'",
    "just-preview": "vite preview",
    "preview": "$npm_execpath run build && $npm_execpath run just-preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.30.1",
    "@jest/globals": "30.0.4",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-window": "1.8.8",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "@vitejs/plugin-react": "4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "9.30.1",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "npm-check-updates": "18.0.1",
    "prettier": "3.6.2",
    "stylelint": "16.21.1",
    "stylelint-config-standard": "38.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vite": "7.0.2",
    "vitest": "3.2.4",
    "web-vitals": "5.0.3"
  },
  "packageManager": "pnpm@10.12.4"
